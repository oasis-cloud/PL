# functions are introduced with `lambda` or `λ`
fib = lamdba (n) if n < 2 then n else fib(n-1) + fib(n-2);

println(fib(15));

print-range = λ(a, b)
                if a<= b then {
                    print(a);
                    if a + 1 <= b {
                        print(", ");
                        print-range(a+1, b);
                    } else println("");
                };
print-range(1, 5);
